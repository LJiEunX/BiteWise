							"listen": "test",
							"script": {
								"exec": [
									"var responseCode = pm.response.code;\r",
									"if (responseCode >= 200 && responseCode < 300) {\r",
									"    var responseBody = pm.response.json();\r",
									"    var token = responseBody.data.token;\r",
									"    pm.environment.set(\"auth_token\", token);\r",
									"} else {\r",
									"    console.log(\"Failed to get token. Response code: \" + responseCode);\r",
									"}"
								],
